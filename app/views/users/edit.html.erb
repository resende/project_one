<div class="edit_profile">

<%= form_for(@user) do |f| %>

  <%= f.label :name %>
   <%= f.text_field :name %>
<br>
  <%= f.label :image %><br />
  <%= f.file_field :image %>
<br>
  <%= f.label :about %><br>
  <%= f.text_field :about %>
<br>
  <%= f.label :contact %><br>
   <%= f.text_field :contact %>
<br>
   <%= f.label :genre %><br>
   <%= f.text_field :genre %>
<br> 

<li><%= link_to 'Manage Songs', tracks_path %></li> 

<%= f.submit %>
<%= link_to 'Cancel', root_path %>
<% end %>

